<Window x:Class="PaperFactory.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PaperFactory"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Одежда для ручек" Height="500" Width="850" MinHeight="500" MinWidth="850" Closing="Window_Closing">
    <Grid Background="#FFFFFF">
        <Grid.RowDefinitions>
            <RowDefinition Height="88"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Margin="0,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Center" Content="Фабрика СИЗ 'Одежда для ручек'" FontFamily="Candara" FontSize="20"/>
        <Frame Grid.Row="1" Name="MainFrame" NavigationUIVisibility="Hidden" />
    </Grid>
</Window>
